// AULA 57
// interpolation
const nome = 'maria';
const idade = 18;
const resultado = 'Esta é a ' + nome + ' e sua idade é ' + idade;
console.log(resultado);

const valor = `Esta é a ${nome} e sua idade é ${idade}`;
console.log(valor);
const conta = `O resultado de 4 + 6 é: ${4 + 6}`;
console.log(conta);
